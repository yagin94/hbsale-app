    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "start": "electron .",
    "app:build": "npm run build && npm run start"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/vue-query": "^5.77.2",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.8.4",
    "pinia": "^3.0.1",
    "qs": "^6.14.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.0",
    "@types/qs": "^6.9.18",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^36.2.1",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "^4.1.7",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
